{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@excalidraw/excalidraw": ["./node_modules/@excalidraw/excalidraw/dist/excalidraw.production.min.js"],
      "@excalidraw/excalidraw/types/*": ["./node_modules/@excalidraw/excalidraw/dist/types/*"]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  }
}
